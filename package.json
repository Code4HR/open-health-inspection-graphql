{
  "name": "open-health-inspection-graphql",
  "version": "0.0.1",
  "description": "GraphQL server using webpack and typescript",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:notify": "npm run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify",
    "start": "npm run build && node dist/main.js",
    "start:watch": "concurrently \"npm run build:watch\" \"node-dev dist/main.js\"",
    "preupver": "npm test",
    "upver": "standard-version",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "keywords": [
    "apollo",
    "backend",
    "apollo-server",
    "typescript",
    "webpack",
    "graphql",
    "graphiql",
    "express"
  ],
  "author": "Austin Hodge",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "1.16.3",
    "@types/cors": "2.8.1",
    "@types/express": "^4.0.35",
    "@types/helmet": "0.0.35",
    "@types/morgan": "^1.7.32",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "graphql": "0.9.6",
    "graphql-server-express": "^0.7.2",
    "graphql-tools": "0.11.0",
    "helmet": "^3.6.0",
    "lodash-es": "^4.17.4",
    "mongoose": "^4.10.1",
    "morgan": "^1.8.1",
    "node-kmeans": "^1.1.0",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.9.1",
    "@types/jest": "^19.2.3",
    "@types/lodash-es": "^4.14.5",
    "@types/mongodb": "^2.2.2",
    "@types/mongoose": "^4.7.13",
    "@types/node": "^7.0.18",
    "awesome-typescript-loader": "^3.1.3",
    "concurrently": "^3.4.0",
    "jest": "^20.0.1",
    "jest-cli": "^20.0.1",
    "node-dev": "^3.1.3",
    "standard-version": "^4.0.0",
    "ts-jest": "^20.0.3",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.3.2",
    "webpack": "^2.5.1",
    "webpack-node-externals": "^1.6.0"
  },
  "snyk": true
}
